#include <iostream>
using namespace std;
#include<algorithm>
#include<climits>

int main()
{
   int n;
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   int smallest =INT_MAX;
   int largest = INT_MIN;
   for(int i=0;i<n;i++){
       smallest = min(smallest,arr[i]);
       largest = max(largest,arr[i]);
   }
   cout<<smallest<<" "<<largest;

    return 0;
}

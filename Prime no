#include <iostream>
using namespace std;
 int isPrime(int n){
     int count =0;
     for(int i=1;i<=n;i++){
       if(n%i==0){
           count++;
       }  
     }
     return count;
 }
int main()
{
    int n;
    cin>>n;
    
     int cnt = isPrime(n);
     if(cnt>2){
         cout<<"no"<<endl;
     }
     else{
         cout<<"yes"<<endl;
     }

    return 0;
}
